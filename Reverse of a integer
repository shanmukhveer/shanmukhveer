import java.io.*; 
import java.util.*; 
class Solution { 
public static void main(String[] args) 
{ 
int n,r=0;
System.out.print("enter the value=");
Scanner sc = new Scanner( System.in);
 n = sc.nextInt(); 
 while(n!= 0) {
 	 int re = n % 10;
 	  r = r * 10 + re;
 	   n /= 10;
 	    } System.out.println("r="+r);
	  }
}
