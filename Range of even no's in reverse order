import java.util.*;
class Sample
{
	public static void main(String args[])
	{
		int m,n;
		Scanner sc=new Scanner(System.in);
		System.out.print(" enter the value of m :  ");
		m=sc.nextInt();
		System.out.print(" enter the value of n :  ");
		n=sc.nextInt();
		while(m>=n){
			if(m%2==0)
		System.out.println( m);
		m--;
		
		}
	}
}

