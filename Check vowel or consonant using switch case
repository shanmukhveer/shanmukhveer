import java.util.*;
class voc
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		char ch;
		System.out.print("eneter the character:\t");
		 ch=sc.next().charAt(0);
		switch(ch)
		{
			case 'a':
			case 'e':
			case 'i':
			case 'o':
			case 'u':
			case 'A':
			case 'E':
			case 'I':
			case 'O':
			case 'U':System.out.println("\tvowel");
			break;
			default:System.out.println("\tconsonant");
		}
	}
}
